//print_r($query); die;

 $run=mysqli_query($conn,$query);
 
 //$row = mysqli_fetch_assoc($run);
 while ($row = mysqli_fetch_assoc($run))
{
 
 $result[] = $row;
$alias = array_column($result, 'alias');
$inenglish = array_column($result, 'inenglish');
$ingerman = array_column($result, 'ingerman');

print_r($alias);


}
